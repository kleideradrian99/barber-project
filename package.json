{
  "name": "madrigal-project",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build": "tsc -p server && ng build --configuration=production",
    "build-server": "tsc -p server",
    "predev": "npm run build-server",
    "dev": "concurrently \"ng serve --proxy-config proxy.conf.json --open\" \"tsc -w -p server\" \"nodemon dist/server/app.js\"",
    "start": "node dist/server/app.js",
    "start:prod": "npm run build && node dist/server/app.js",
    "dev:server": "tsc -w -p server",
    "client-serve": "ng serve --proxy-config proxy.conf.json --open"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/cdk": "^18.2.14",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/google-maps": "^18.2.14",
    "@angular/material": "^18.2.14",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/router": "^18.2.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "leaflet": "^1.9.4",
    "morgan": "^1.10.1",
    "ngx-leaflet": "^0.0.16",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.19",
    "@angular/cli": "^18.2.19",
    "@angular/compiler-cli": "^18.2.0",
    "@types/express": "^5.0.3",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^24.6.0",
    "concurrently": "^9.2.1",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "~5.5.2"
  }
}
